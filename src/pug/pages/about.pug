extends ../layout

block vars
    - var title = "Обо мне";

block header
    include ../section/header
block content
    section.about
        .about__left
        .about__right
    section.map
        .map__google
            iframe(src="https://www.google.com/maps/embed?pb=!1m14!1m12!1m3!1d1183.7997264858889!2d87.33798673606992!3d53.60060825723373!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!5e0!3m2!1sru!2sru!4v1512794618212" width="100%" height="100%" frameborder="0" style="border:0" allowfullscreen)
            .map__contacts
                h3.subtitle.map__title контакты
                ul.contacts
                    li.contants__item
                        svg.contacts__svg
                            use(xlink:href="#icon-skype")
                        .contacts__text npo100lol
                    li.contants__item
                        svg.contacts__svg
                            use(xlink:href="#icon-envelope")
                        .contacts__text 4-09@bk.ru
                    li.contants__item
                        svg.contacts__svg
                            use(xlink:href="#icon-phone")
                        .contacts__text +7 909 520 84 63
                    li.contants__item
                        svg.contacts__svg
                            use(xlink:href="#icon-map_marker")
                        .contacts__text Кемеровская обл. город Осинники